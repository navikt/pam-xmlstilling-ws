<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%-5level] %d{ISO8601} [%t] %c{1.} - %msg%n" />
        </Console>
    </Appenders>

    <Loggers>
        <Logger name="no.nav" level="debug"/>
        <Root level="info">
            <AppenderRef ref="console" />
        </Root>
    </Loggers>
</Configuration>