<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
	http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
             version="1.0">

   <persistence-unit name="xmlstilling-web-persistence" transaction-type="JTA">
       <jta-data-source>java:jboss/jdbc/sixdb</jta-data-source>
       <non-jta-data-source>java:jboss/jdbc/sixdb</non-jta-data-source>
       <class>no.nav.sbl.entity.six.EksternBrukerEntity</class>
       <class>no.nav.sbl.entity.six.StillingBatchEntity</class>
       <class>no.nav.sbl.entity.six.FeilLoggEntity</class>
       <class>no.nav.sbl.entity.six.XSLTemplateEntity</class>
       <properties>
           <property name="hibernate.dialect" value="org.hibernate.dialect.Oracle10gDialect" />
           <property name="jboss.entity.manager.factory.jndi.name" value="xmlstilling/MyEntityManagerFactory"/>
       </properties>
   </persistence-unit>

</persistence>